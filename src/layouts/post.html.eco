---
layout: default
---

<div class="content">
    <article class="b-post">
        <h1 class="b-post__title">
            <%- @document.title %>
        </h1>

        <div class="b-post__text">
            <%- @content %>
        </div>

        <div class="b-post__meta">
            <span class="b-post__date"><%= @pubDate @document.date %></span>
        </div>
    </article>
</div>
